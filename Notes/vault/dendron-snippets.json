{
	"Swift Snippet": {
		"prefix": ["swift", "ssnippet"],
		"body": [
			"```swift",
			"${0:Add your code block here}",
			"```"
		],
		"description": "Code Snippet formatted for Swift Programming Language"
	},

	"Bold Title": {
		"prefix": ["boldtitle", "bt"],
		"body": [
			"# **`</> ${0: Bold Title}`**"
		],
		"description": "for Bold Title"
	},

	"Bold Section Title": {
		"prefix": ["boldsection", "bst"],
		"body": [
			"## **${0:Bold Section Title}**"
		],
		"description": "for Bold Section Title format"
	},

	"Regular Sub-Section Title": {
		"prefix": ["regularsubsection", "rsst"],
		"body": [
			"### ${0:Regular Sub-Section Title}"
		],
		"description": "for Regular Sub-Section Title format"
	},

	"Regular Hightlight Sub-Section Title": {
		"prefix": ["regularsubsection", "rhsst"],
		"body": [
			"### `${0:Regular Hightlight Sub-Section Title}`"
		],
		"description": "for Regular Hightlight Sub-Section Title format"
	},

	"Bold Sub-Section Title": {
		"prefix": ["boldsubsection", "bsst"],
		"body": [
			"### **${0:Bold Sub-Section Title}**"
		],
		"description": "for Bold Sub-Section Title format"
	},

	"Bold Hightlight Sub-Section Title": {
		"prefix": ["boldsubsection", "bhsst"],
		"body": [
			"### **`${0:Bold Hightlight Sub-Section Title}`**"
		],
		"description": "for Bold Hightlight Sub-Section Title format"
	},

	"Regular Highlight Text": {
		"prefix": ["regularhighlight", "rht"],
		"body": [
			"`${0:Regular Highlight Text}`"
		],
		"description": "for Regular Highlight Text format"
	},
	
	"Bold Highlight Text": {
		"prefix": ["boldhighlight", "bht"],
		"body": [
			"**`${0:Bold Highlight Text}`**"
		],
		"description": "for Bold Highlight Text format"
	}
}